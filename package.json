  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "postbuild": "if [[  -z $CI ]];  then (rimraf ../hole-backend/public &&ncp dist ../hole-backend/public) fi",
    "clean": "rimraf dist/",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --host 0.0.0.0  --port 8080 --progress  --no-info",
    "dev": "npm run lint-ts && concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-css": "stylelint --syntax less \"./src/**/*.less\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --ignore-path .gitignore",
    "postinstall": "",
    "pretest": "npm run lint",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false",
    "posttest": "istanbul check-coverage --statement 75 --branch 50 --function 50"
  },
